#!/bin/sh
if [ -z "${PYICF_SHELL+x}" ];then
    printf "###############################\n"
    printf "#Entering py-icf dev shell env#\n"
    printf "###############################\n"

    export PYICF_SHELL=1
    # export PATH_BACKUP=${PATH}
    export PYTHONPATH=@CMAKE_BINARY_DIR@/icf
    # export PATH=@CMAKE_BINARY_DIR@:@CMAKE_BINARY_DIR@/bin:
    export PS1_T_BACKUP=${PS1}
    PS1="(py-icf dev)"${PS1}
else
    unset PYICF_SHELL
    PS1=${PS1_T_BACKUP}
    # PATH=${PATH_BACKUP}
    # unset PATH_BACKUP
    unset PYTHONPATH
    unset PS1_T_BACKUP
    echo "Exited py-icf dev shell Environment."
fi